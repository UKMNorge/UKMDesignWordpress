{% extends "UKMDesign/Layout/base.html.twig"|UKMpath %}

{% block content %}
	<div class="container">
		<div class="row">
			<div class="col-12">
				{{ page.content|raw }}

				{% for id, element in UKMDesign.getConfig('logoer') %}
					<h2 class="mt-5">{{ UKMDesign.getConfig('logoer.'~ id ~'.navn') }}</h2>
					{% include "UKMDesignBundle:GrafiskProfil:Element.html.twig"|UKMpath  with { element: element } %}
				{% endfor %}
			</div>
		</div>
	</div>
{% endblock %}