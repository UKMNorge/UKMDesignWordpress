<div class="list-unstyled" id="arrangementListe{{id|capitalize}}">
    {% for arrangement in arrangementer|sort((arr1, arr2) => arr1.getStart() > arr2.getStart()) %}
        <div class="clearfix mb-3" style="{{ arrangement.erFerdig() ? 'opacity: .65' }}">
            {% include "Arrangement/ListeElement.html.twig" %}
        </div>
    {% else %}
        <div>{{ omrade.getNavn() }} har ingen arrangement.</div>
    {% endfor %}

</div>