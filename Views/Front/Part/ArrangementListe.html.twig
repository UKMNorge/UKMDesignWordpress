<ol class="list-unstyled">
    {% for arrangement in arrangementer|filter(arrangement => arrangement.erRegistrert() and arrangement.erSynlig()) %}
        <li class="clearfix mb-3" style="{{ arrangement.erFerdig() ? 'opacity: .65' }}">
            {% include "Arrangement/ListeElement.html.twig" %}
        </li>
    {% else %}
        <li>{{ omrade.getNavn() }} har ingen arrangement.</li>
    {% endfor %}
</ol>